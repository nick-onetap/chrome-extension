{
    "name": "Salmonware Chrome Extension",
    "version": "1.0.0",
    "manifest_version": 3,
    "description": "Your NEW favorite salmon extension",
    "permissions": ["activeTab"],
    "action": {
    "default_popup": "popup.html", 
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
  }
}